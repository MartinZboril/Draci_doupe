<Application x:Class="Draci_doupe.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Draci_doupe"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="Medieval">pack://application:,,,/Fonts/#MedievalSharp</FontFamily>
        <BitmapImage x:Key="Scroll" UriSource="Resources/Scroll.png"></BitmapImage>
        <BitmapImage x:Key="Barbarians" UriSource="Images/Barbarians.jpg"></BitmapImage>
        <BitmapImage x:Key="Bear" UriSource="Images/Bear.png"></BitmapImage>
        <BitmapImage x:Key="Boss" UriSource="Images/Boss.png"></BitmapImage>
        <BitmapImage x:Key="Pirate" UriSource="Images/Pirate.png"></BitmapImage>
        <BitmapImage x:Key="Player" UriSource="Images/player.png"></BitmapImage>
        <BitmapImage x:Key="Background" UriSource="Images/Background.jpg"></BitmapImage>
        <BitmapImage x:Key="Background1" UriSource="Resources/Background1.png"></BitmapImage>
    </Application.Resources>
</Application>
